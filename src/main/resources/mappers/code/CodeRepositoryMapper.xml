<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.ginong.web.repository.code.CodeRepository">
    <select id="find" resultType="CodeDetail">
        SELECT *
        FROM CODE_DETAIL
        WHERE GROUP_CD = #{groupCode}
    </select>
</mapper>